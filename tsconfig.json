{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "rollup/parseAst": ["node_modules/rollup/dist/parseAst.d.ts"]
    },
    "inlineSourceMap": true,
    "inlineSources": true,
  "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
  "moduleResolution": "node",
  "skipLibCheck": true,
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}
